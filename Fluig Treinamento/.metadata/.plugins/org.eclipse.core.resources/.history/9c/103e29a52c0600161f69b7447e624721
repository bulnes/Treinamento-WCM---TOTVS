var MyWidget = SuperWidget.extend({
	
    init: function() {
    },
    
    bindings: {
        local: {
        	"openmodal": ["click_openModal"]
        },
        global: {}
    },
    
    openModal: function() {
    	var myModal = FLUIGC.modal({
    	    title: 'title',
    	    content: {
    	        widgetCode: 'brunohelloworld', 
    	        ftl: 'view.ftl',
    	        data: {}
    	    },
    	    id: 'fluig-modal'
    	});
    },
});