var brunoapiuser = SuperWidget.extend({
	editMode: false,
	viewMode: false,
	
    init: function() {
    	if (this.viewMode) {
    		
    		$.ajax({
    			beforeSend: function() {
    				
    			},
    			complete: function() {
    				
    			},
    			error: function() {
    				
    			},
    			success: function(users) {
    				console.log(users);
    			},
    			type: "GET",
    			url: "/api/public/social/user"
    		});
    		
    	}
    },
    
    bindings: {
        local: {},
        global: {}
    },
});